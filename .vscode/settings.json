{
	"files.associations": {
		"*.tiny": "yaml",
		"*.yaml.txt": "yaml",
		"*.js.txt": "javascript",
		"*.meta": "yaml",
		"*.manifest": "yaml",
		"*.asset": "yaml",
		"project.tiny": "yaml"
	},
	"files.exclude": {
		"Library": true,
		"Logs": true,
		"Temp": true,
		"ProjectSettings": true,
		"UserSettings": true,
		"node_modules": true,
		"qrcodes": true,
		"obj": true,
		"*.csproj": true,
		"*.sln": true
	},
	"files.eol": "\n",
	"editor.tabSize": 4,
	"npm.packageManager": "yarn",
	"eslint.validate": [ "javascript", "javascriptreact", "typescript", "typescriptreact" ],
	"omnisharp.useModernNet": false,
	"dotnet.defaultSolution": "ts-game-kit.sln",
	"editor.codeActionsOnSave": { "source.fixAll.eslint": true },
	"typescript.tsdk": "node_modules/typescript/lib",
	"dotnet.preferCSharpExtension": true,
}
